# http://editorconfig.org

root = true

[*]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
insert_final_newline = true
charset = utf-8
end_of_line = lf

[*.py]
indent_size = 4

[Makefile]
indent_style = tab

[settings]
known_third_party = asgi_lifespan,bs4,bson,docs,fastapi,geopy,httpx,hydralit,hydralit_components,mailjet_rest,matplotlib,models,motor,pandas,pdfkit,pydantic,pydeck,pytest,requests,routers,sentry_sdk,settings,streamlit,utils
